{
  "name": "stylelint-no-indistinguishable-colors",
  "version": "2.3.1",
  "description": "Stylelint plugin to add rule no-indistinguishable-colors",
  "keywords": [
    "stylelint-plugin"
  ],
  "main": "lib/index.js",
  "files": [
    "lib/"
  ],
  "author": "Irina Sokolovskaja <sokolovskaja.irina@gmail.com>",
  "homepage": "https://github.com/ierhyna/stylelint-no-indistinguishable-colors",
  "bugs": {
    "url": "https://github.com/ierhyna/stylelint-no-indistinguishable-colors/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ierhyna/stylelint-no-indistinguishable-colors.git"
  },
  "license": "MIT",
  "scripts": {
    "prettier": "prettier --config prettier.config.mjs --check lib/*.js test/*.js eslint.config.mjs prettier.config.mjs",
    "prettier-fix": "prettier --config prettier.config.mjs --write lib/*.js test/*.js eslint.config.mjs prettier.config.mjs",
    "eslint": "eslint . --config eslint.config.mjs",
    "eslint-fix": "eslint . --config eslint.config.mjs --fix",
    "test": "node --test test/index.js"
  },
  "dependencies": {
    "postcss": "^8.5.3",
    "postcss-colorguard": "^2.0.29"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "eslint": "^10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-promise": "^7.2.1",
    "globals": "^17.3.0",
    "prettier": "^3.8.1",
    "prettier-eslint": "^16.4.2",
    "stylelint": "^17.2.0",
    "stylelint-test-rule-node": "^1.0.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.55.0"
  },
  "peerDependencies": {
    "stylelint": "^17.2.0"
  },
  "type": "module",
  "overrides": {
    "eslint": "$eslint"
  },
  "engines": {
    "node": "^20.19.0 || ^22.12.0 || >= 24.0.0"
  }
}
